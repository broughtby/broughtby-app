{
  "name": "broughtby-app",
  "version": "1.0.0",
  "description": "Premium marketplace connecting brands with vetted brand ambassadors",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "db:migrate": "node server/db/migrate.js",
    "db:seed": "node server/db/seed.js",
    "db:clean": "node server/db/clean-test-data.js",
    "db:reset-user": "node server/db/reset-user-data.js",
    "install:all": "npm install && cd client && npm install"
  },
  "keywords": [
    "marketplace",
    "brand-ambassadors",
    "matching"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "pg": "^8.11.3",
    "resend": "^6.3.0",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
